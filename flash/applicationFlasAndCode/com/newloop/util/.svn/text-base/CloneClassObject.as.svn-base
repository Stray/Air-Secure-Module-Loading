*****************************************************************************/package com.newloop.util{    import flash.text.TextFormat;    import flash.text.TextField;	import flash.text.StyleSheet;	public final class CloneClassObject {		public static function cloneTextFormat(textFormat:TextFormat):TextFormat {			var txtField:TextField = new TextField();			txtField.text = "A";			txtField.setTextFormat(textFormat);			var newFormat:TextFormat = txtField.getTextFormat();            return newFormat;        }		//		public static function cloneStyleSheet(styleSheet:StyleSheet):StyleSheet {						var styleNames:Array = styleSheet.styleNames;			var iLength:uint = styleNames.length;			var newStyleSheet:StyleSheet = new StyleSheet();			for (var i:uint = 0; i < iLength; i++) {				newStyleSheet.setStyle(styleNames[i], styleSheet.getStyle(styleNames[i]));			}            return newStyleSheet;        }		//	}	//}